
<launch>
    <arg name="ip" default="192.168.100.103"/>
    <arg name="calibration_index" default="-1"/>
    <arg name="frame" default="/ati_link"/>
    
    <param name="tip_link" value="ati_link"/>
    
    <node name="deployer" pkg="rtt_ros" type="deployer" args="-s $(find @PROJECT_NAME@)/scripts/run_hw_standalone.ops --" output="screen">
        <param name="ip" value="$(arg ip)"/>
        <param name="calibration_index" value="$(arg calibration_index)"/>
        <param name="frame" value="$(arg frame)"/>
    </node>
</launch>

